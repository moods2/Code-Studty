  	<servlet-name>del</servlet-name>
	<url-pattern>/del.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>edit</servlet-name>
	<servlet-class>com.test.address.Edit</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>edit</servlet-name>
	<url-pattern>/edit.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>editok</servlet-name>
	<servlet-class>com.test.address.EditOk</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>editok</servlet-name>
	<url-pattern>/editok.do</url-pattern>
  </servlet-mapping>

</web-app>
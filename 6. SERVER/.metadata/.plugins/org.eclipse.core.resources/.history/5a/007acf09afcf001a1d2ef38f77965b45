package com.test.servlet;

//알바생 -> 요청에 따른 페이지를 동적으로 만드는 사람
public class Test {
	
	public static void main(String[] args) {
		
		//내 받은 편지함 페이지
		String html = "";
		
		html += "<html>";
		html += "<head>";
		html += "<meta charset='UTF-8'>";
		html += "<title>받은 편지함</title>";
		html += "</head>";
		html += "<body>";
		html += "<h1>받은 편지함</h1>";
		html += "<p>메일이 도착했습니다.</p>";
		html += "</body>";
		html += "</html>";
		
	}
	
}
